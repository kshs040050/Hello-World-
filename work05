#include <iostream>
#include <cstdlib>

using namespace std;

int main() {
    int x = 3,y = 4;
    cout << boolalpha; //輸出布林值true/false
    cout << "x = " << x << ", y = " << y << endl;
    cout << "-------------------"<<endl;
    cout << "x > y : " << (x>y) << endl;
    cout << "x < y : " << (x<y) << endl;
    cout << "x >= y : " << (x>=y) << endl;
    cout << "x <= y : " << (x<=y) << endl;
    cout << "x == y : " << (x==y) << endl; //
    cout << "x != y : " << (x!=y) << endl;
    system("pause");
    return 0;

}
